<?xml version="1.0" encoding="utf-8" ?> 
<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense">
  <SFMControlPanel>
    <FormPage Application="SFM Control Panel">
      <CodeBeside Type="Sitecore.Feedback.Module.PresentationLayer.ControlPanel.SFMControlPanelForm,Sitecore.Feedback.Module"/>
      
      <Scrollbox ID="ControlPanel" Border="none" Padding="0">
        <TaskBackground>
          <TaskTitle ID="Title"><Literal ID="TitleText"/></TaskTitle>

          <TaskOptions ID="OptionsSection">
            <Border ID="Options"/>
          </TaskOptions>
          
          <TaskLinks ID="LinksSection">
            <Border ID="Links"/>
          </TaskLinks>
          
        </TaskBackground>
      </Scrollbox>
    
    </FormPage>
  </SFMControlPanel>
</control>